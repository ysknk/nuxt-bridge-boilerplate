{
  "name": "nuxt-bridge-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "start": "nuxt start",
    "build": "nuxt generate",
    "generate": "NUXT_ENV=\"development\" npm run build",
    "dev:staging": "cross-env NUXT_ENV=\"staging\" npm run dev",
    "generate:staging": "cross-env NUXT_ENV=\"staging\" npm run build",
    "dev:production": "cross-env NUXT_ENV=\"production\" npm run dev",
    "generate:production": "cross-env NUXT_ENV=\"production\" npm run build",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.19.3",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@^3.0.0-27672061.a645f7f",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vueuse/head": "^0.7.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^9.4.0",
    "nib": "^1.2.0",
    "nuxt-edge": "^2.16.0-27616340.013f051b",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylus": "^0.59.0",
    "stylus-loader": "^3.0.2"
  },
  "volta": {
    "node": "16.17.0",
    "npm": "8.18.0"
  }
}
